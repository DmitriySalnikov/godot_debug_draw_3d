/.gitignore				export-ignore
/.gitmodules			export-ignore
/.github				export-ignore
/LICENSE				export-ignore
/SConstruct				export-ignore
/*.*					export-ignore
/src	        		export-ignore
/godot-cpp				export-ignore
/images					export-ignore
/jni					export-ignore
/patches				export-ignore