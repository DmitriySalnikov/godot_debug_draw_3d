{
  "configurations": [
    {
      "name": "DefaultCPP",
      "includePath": [
        "${workspaceFolder}/src/**",
        "${workspaceFolder}/godot-cpp/gdextension/**",
        "${workspaceFolder}/godot-cpp/include",
        "${workspaceFolder}/godot-cpp/gen/include"
      ],
      "defines": [
        "DEBUG_ENABLED",
        "DEV_ENABLED",
        "UNICODE",
        "_UNICODE",
        "GDEXTENSION_LIBRARY",
        "NOMINMAX",
        "TYPED_METHOD_BIND",
        "HOT_RELOAD_ENABLED",
        "TOOLS_ENABLED",
        "TRACY_ENABLE",
        "TRACY_ON_DEMAND",
        "TRACY_DELAYED_INIT",
        "TRACY_MANUAL_LIFETIME",
        "TELEMETRY_ENABLED",
        "TELEMETRY_APP_ID=\"\"",
        "TELEMETRY_HOST=\"\"",
        "TELEMETRY_DST_FILE_KEY=\"\"",
        //"REAL_T_IS_DOUBLE",
      ],
      "windowsSdkVersion": "10.0.22000.0",
      "compilerPath": "cl.exe",
      "cStandard": "c17",
      "cppStandard": "c++17",
      "intelliSenseMode": "windows-msvc-x64"
    }
  ],
  "version": 4
}